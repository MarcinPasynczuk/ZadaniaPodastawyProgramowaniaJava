import java.util.Scanner;
public class Main
{
    public static void main(String args[]) {
        System.out.print("Wczytaj liczbę całkowitą aby obliczyć ilość jej dzienników.");
        Scanner input = new Scanner(System.in);
        int i = input.nextInt();


        int number;
        int number2 = 0;
        int number3 = 0;

        for(number=i;number>0;number--) {
            if (i%number==0) {
                number3++;
                }
                if (number<1) break;
            }
        System.out.println(number3);

        }




        }
