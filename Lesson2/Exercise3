import java.util.Scanner;
public class Main
{
    public static void main(String args[]) {
        System.out.print("Wczytaj liczbę całkowitą aby obliczyć ilość jej dzienników.");
        Scanner input = new Scanner(System.in);
        int i = input.nextInt();


        int number = i;
        int number2 = 0;
        int number3 = 0;

        do {
            number2 = i%number--;
            if (number2==0) {
                System.out.println(number3++);
            }
        }


        while (number>0);
    }
}
