import java.util.Scanner;
public class Main
{
    public static void main(String args[]) {
        System.out.print("Wpisz liczbę od której program zacznie wypisywać liczby do 1000. ");
        Scanner input = new Scanner(System.in);
        int i = input.nextInt();


        int number;
        int number2 = 0;
        int number3 = 0;

        for(number=i;number>0;number++) {
            if (i < 1000) {
                System.out.println(number);
                }
                if (number>=1000) break;
            }


        }
        
        }
