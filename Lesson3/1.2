import java.util.Scanner;
public class Exercise {
    public static void main(String[] args)
    {
        Scanner sc = new Scanner (System.in);
        int x=1, y=1;
        System.out.print("Enter rows ");
        x = sc.nextInt();
        System.out.print("Enter columns ");
        y = sc.nextInt();

        for (int i=1; i<=x; i++)
        {
            for (int j=1; j<=y; j++)
            {
                int num = i*j;
                System.out.printf("%5d", num);
            }
            System.out.println();
        }
    }
}
